#! /bin/bash

#shell script for running bowtie2 on my samples, and aligning them to the unmasked Hv draft genome.

cd ~/temp

#LA1997 samples
bowtie2 -x ./genome/bowtie2_build/Hvir -q ./samples_1997/LA1997_10.fq -S ./bowtie_output/LA1997_10.sam --end-to-end --very-sensitive --no-unal
